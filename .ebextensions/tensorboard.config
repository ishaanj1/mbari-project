files:
  /etc/nginx/conf.d/tensorboard.conf:
    mode: "000644"
    owner: root
    group: root
    content: |
      server {
          listen 8080;
          server_name tensorboard.deepseaannotations.com;

          location / {
              proxy_pass http://localhost:6008;
          }   
      }

packages:
  python:
    tensorboard: '1.14.0'
